{% if data and ((data.value is defined and data.value) or (data.to is defined and data.to)) %}
    <button type="button" class="btn disabled btn-sm btn-primary mt-2 mr-2 mt-md-1">
        {{ filtertype.filterLabel|trans }}

        {% block value %}
            <span class="badge bg-secondary">
                {{ data.value is defined ? data.value|date('d/m/Y') : '...' }}
                -
                {{ data.to is defined ? data.to|date('d/m/Y') : '...' }}
            </span>
        {% endblock %}
    </button>
{% endif %}
