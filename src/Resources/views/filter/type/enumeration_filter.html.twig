<select name="{{nameprefix}}comparator{{namesuffix}}">
    <option value="in" {% if data.comparator is defined and data.comparator == "in" %} selected="selected" {% endif %} >{{ 'filter.enumeration.contains' | trans({}, 'EasyAdminPlusBundle') }}</option>
    <option value="notin" {% if data.comparator is defined and data.comparator == "notin" %} selected="selected" {% endif %} >{{ 'filter.enumeration.does_not_contain' | trans({}, 'EasyAdminPlusBundle') }}</option>
</select>

<select name="{{nameprefix}}value{{namesuffix}}[]" size="4" multiple>
{% for key,option in options %}
    <option value="{{ key }}"{% if data.value is defined and key in data.value %} selected{% endif %}>{{ option|trans }}</option>
{% endfor %}
</select>
