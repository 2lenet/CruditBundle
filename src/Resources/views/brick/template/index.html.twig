{% if view.role is not defined or not view.role or is_granted(view.role) %}
    {% if view.data.title %}
        <h2 class="{% if view.data.titleCss %}{{ view.data.titleCss }}{% endif %}">{{ view.data.title|trans }}</h2>
        <hr/>
    {% endif %}
    {% include view.config.template with {'resource':view.data.resource} %}
{% endif %}
