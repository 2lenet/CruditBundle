<div class="row">
        <div class="col-md-12 align-right mb-2">
            {%  for action in view.data.actions %}
                <a class="btn btn-primary float-right ml-2" href="{{ action.url ?? path(action.path.route, action.path.params) }}">{{ action.label|trans }}</a>
            {%  endfor %}
        </div>
</div>
