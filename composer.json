{
    "name": "2lenet/crudit-bundle",
    "type": "symfony-bundle",
    "description": "The easy like Crud'it Bundle.",
    "keywords": ["admin","backend","symfony","crud","generator"],
    "license": "MIT",
    "homepage": "https://github.com/2lenet/CruditBundle",
    "authors": [
        {
            "name": "Project Contributors",
            "homepage": "https://github.com/2lenet/CruditBundle/graphs/contributors"
        }
