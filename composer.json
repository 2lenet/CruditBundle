    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^4.4 || ^5.2 || ^6.0",
        "symfony/property-info": "~3.0|^4.0|^5.0|^6.0",
        "symfony/property-access": "~3.0|^4.0|^5.0|^6.0",
        "phpdocumentor/reflection-docblock": "~3.0|^4.0|^5.0",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "symfony/security-core": "~3.0|^4.0|^5.0|^6.0",
        "symfony/serializer": "~3.0|^4.0|^5.0|^6.0",
        "symfony/form": "~3.0|^4.0|^5.0|^6.0",
        "symfony/dependency-injection": "^4.4|^5.0|^6.0",
        "symfony/options-resolver": "^4.4|^5.0|^6.0",
        "doctrine/common": "^2.13 || ^3.0",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^1.12|^2.0",
        "doctrine/orm": "^2.6,>=2.6.3",
        "doctrine/persistence": "^1.3.3 || ^2.0",
        "symfony/twig-bundle": "^5.2|^6.0",
        "symfony/translation": "^5.2|^6.0",
        "phpoffice/phpspreadsheet": "^1.18",
        "symfony/workflow": "^5.2|^6.0",
        "gedmo/doctrine-extensions": "^3.1"
    },
    "require-dev": {
        "ergebnis/phpstan-rules": "^0.15.3",
        "phpstan/phpstan": "^0.12.54",
        "phpstan/phpstan-mockery": "^0.12.3",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpstan/phpstan-doctrine": "^0.12.22",
        "phpstan/phpstan-symfony": "^0.12.11",
        "squizlabs/php_codesniffer": "^3.5",
        "slevomat/coding-standard": "^6.4",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "symfony/maker-bundle": "^1.0",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Lle\\CruditBundle\\": "src/"
        }
    }
